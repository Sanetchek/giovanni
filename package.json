{
  "name": "underscores",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called giovanni, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "type": "module",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/giovanni#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/giovanni.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/giovanni/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^19.2.2",
    "autoprefixer": "^10.4.19",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "rtlcss": "^3.5.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "minify:js": "gulp minify-js",
    "lint:js": "wp-scripts lint-js assets/js/*.js",
    "lint:css": "wp-scripts lint-style assets/css/*.css",
    "compile:rtl": "rtlcss style-rtl.css style.css",
    "start": "gulp"
  },
  "dependencies": {
    "jquery-validation": "^1.19.5",
    "jspdf": "^2.5.1"
  }
